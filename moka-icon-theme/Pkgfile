# Description: Moka Icon Theme
# URL: https://github.com/snwh/moka-icon-theme
# Maintainer: Tim Biermann, tbier at posteo dot de
# Depends on:

name=moka-icon-theme
version=git
release=1
source=()
git=(https://github.com/snwh/moka-icon-theme.git)

build() {
    git clone $git $name
    cd $name
    ./autogen.sh
    make
    make DESTDIR=$PKG install
}
