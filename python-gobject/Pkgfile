# Depends on:

name=python-gobject
version=3.10.2
release=1
source=(http://ftp.gnome.org/pub/GNOME/sources/pygobject/3.10/pygobject-3.10.2.tar.xz)

build() {
    cd pygobject-$version
    ./configure --prefix="/usr"
    make
    make DESTDIR=$PKG install

}
